<!DOCTYPE HTML>
<html>
	<head>
		<title>Motors Dynamic</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets/bootstrap/css/bootstrap.min.css" />
		<link rel="stylesheet" href="assets/css/main.css" />
		<noscript><link rel="stylesheet" href="assets/css/noscript.css" /></noscript>
	</head>
	<body class="is-preload">

		<!-- Wrapper -->
			<div id="wrapper">

				<%- include("partials/header"); -%>

				<%- include("partials/menu"); -%>

				<!-- Main -->
					<div id="main" class="alt">

						<!-- One -->
							<section id="one">
								<div class="inner">
									<header class="major">
										<h1>Our Team Members</h1>
									</header>
									
									<div class="row">

										<% teams.forEach(team => { %>
											
										<div class="col-md-3 col-sm-6 co-xs-12 text-center">
											<img src=" <%= team.picturePath %>" class="img-responsive" alt="">	

											<h3> <%= team.name %></h3>

											<h4><em> <%= team.title %> </em></h4>

											<ul class="icons">
												<li><a href="https://www.linkedin.com/in/pektasmehmet/" class="icon alt fa-twitter"><span class="label">Twitter</span></a></li>
												<li><a href="https://www.linkedin.com/in/pektasmehmet/" class="icon alt fa-linkedin"><span class="label">LinkedIn</span></a></li>
											</ul>
										</div>

										<%	}); %>

									</div>
								</div>
							</section>
					</div>

                <%- include("partials/footer"); -%>

			</div>
	</body>
</html>